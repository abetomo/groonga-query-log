<html>
  <head>
    <title>groonga query analyzer</title>
    <style>
table,
table tr,
table tr th,
table tr td
{
  border: 1px solid black;
}

span.slow
{
  color: red;
}

div.parameters
{
  float: left;
  padding: 2em;
}

div.parameters h3
{
  text-align: center;
}

div.parameters table
{
  margin-right: auto;
  margin-left: auto;
}

div.statistics
{
  clear: both;
}

td.elapsed,
td.ratio,
td.n,
td.n-records
{
  text-align: right;
}

td.name
{
  text-align: center;
}
    </style>
  </head>
  <body>
